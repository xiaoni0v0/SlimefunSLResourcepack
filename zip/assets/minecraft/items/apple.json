{
  "model": {
    "type": "select",
    "property": "component",
    "component": "custom_data",
    "cases": [
      {
        "when": {
          "PublicBukkitValues":{
            "slimefun:slimefun_item":"CHRISTMAS_CARAMEL_APPLE"
          }
        },
        "model": {
          "type": "model",
          "model": "slimefun:slimefun/christmas/christmas_caramel_apple"
        }
      },
      {
        "when": {
          "PublicBukkitValues":{
            "slimefun:slimefun_item":"CHRISTMAS_CHOCOLATE_APPLE"
          }
        },
        "model": {
          "type": "model",
          "model": "slimefun:slimefun/christmas/christmas_chocolate_apple"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/apple"
    }
  }
}