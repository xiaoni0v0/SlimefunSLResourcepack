        }
      },
      {
        "when": {"PublicBukkitValues":{"slimefun:slimefun_item":"TINKERS_ARMOUR_TABLE"}},
        "model": {
          "type": "model",
          "model": "slimetinker:workstations/tinkers_armour_table"
