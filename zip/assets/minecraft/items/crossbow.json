{
  "model": {
    "type": "select",
    "property": "component",
    "component": "custom_data",
    "cases": [
      {
        "when": {"PublicBukkitValues":{"slimefun:slimefun_item":"IE_INFINITY_CROSSBOW"}},
        "model": {
          "type": "condition",
          "property": "using_item",
          "on_true": {
            "type": "range_dispatch",
            "property": "crossbow/pull",
            "entries": [
              {
                  "threshold": 0.58,
                  "model": {
                    "type": "model",
                    "model": "infinityexpansion2:gear/star_shooter/infinity_crossbow_1"
                  }
                },
                {
                  "threshold": 1.0,
                  "model": {
                    "type": "model",
                    "model": "infinityexpansion2:gear/star_shooter/infinity_crossbow_2"
                  }
                }
              ],
            "fallback": {
              "type": "model",
              "model": "infinityexpansion2:gear/star_shooter/infinity_crossbow_0"
            }
          },
          "on_false": {
            "type": "select",
            "property": "charge_type",
            "cases": [
              {
                "when": "arrow",
                "model": {
                  "type": "model",
                  "model": "infinityexpansion2:gear/star_shooter/infinity_crossbow_charged"
                }
              },
              {
                "when": "rocket",
                "model": {
                  "type": "model",
                  "model": "infinityexpansion2:gear/star_shooter/infinity_crossbow_firework"
                }
              }
            ],
            "fallback": {
              "type": "model",
              "model": "infinityexpansion2:gear/star_shooter/infinity_crossbow"
            }
          }
        }
      }
    ],
    "fallback": {
      "type": "condition",
      "property": "using_item",
      "on_true": {
        "type": "range_dispatch",
        "property": "crossbow/pull",
        "entries": [
          {
            "threshold": 0.58,
            "model": {
              "type": "model",
              "model": "item/crossbow_pulling_1"
            }
          },
          {
            "threshold": 1.0,
            "model": {
              "type": "model",
              "model": "item/crossbow_pulling_2"
            }
          }
        ],
        "fallback": {
          "type": "model",
          "model": "item/crossbow_pulling_0"
        }
      },
      "on_false": {
        "type": "select",
        "property": "charge_type",
        "cases": [
          {
            "when": "arrow",
            "model": {
              "type": "model",
              "model": "item/crossbow_arrow"
            }
          },
          {
            "when": "rocket",
            "model": {
              "type": "model",
              "model": "item/crossbow_firework"
            }
          }
        ],
        "fallback": {
          "type": "model",
          "model": "item/crossbow"
        }
      }
    }
  }
}
  