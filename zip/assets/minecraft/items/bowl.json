{
  "model": {
    "type": "select",
    "property": "component",
    "component": "custom_data",
    "cases": [
      {
        "when": {"PublicBukkitValues":{"slimefun:slimefun_item":"GOLD_PAN"}},
        "model": {
          "type": "model",
          "model": "slimefun:slimefun/tools/gold_pan"
        }
      },
      {
        "when": {"PublicBukkitValues":{"slimefun:slimefun_item":"NETHER_GOLD_PAN"}},
        "model": {
          "type": "model",
          "model": "slimefun:slimefun/tools/nether_gold_pan"
        }
      },
      {
        "when": {"PublicBukkitValues":{"slimefun:slimefun_item":"AUTOMATED_PANNING_MACHINE"}},
        "model": {
          "type": "model",
          "model": "slimefun:slimefun/basic_machines/automated_panning_machine"
        }
      },
      {
        "when": {"PublicBukkitValues":{"slimefun:slimefun_item":"GN_MORTAR_AND_PESTLE"}},
        "model": {
          "type": "model",
          "model": "gastronomicon:tools/gn_mortar_and_pestle"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/bowl"
    }
  }
}