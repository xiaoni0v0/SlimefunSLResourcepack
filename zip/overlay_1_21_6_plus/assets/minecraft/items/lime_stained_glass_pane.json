{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:component",
    "component": "minecraft:custom_data",
    "cases": [
      {
        "when": {"PublicBukkitValues":{"slimefun:slimefun_item":"_UI_PREVIOUS_ACTIVE"}},
        "model": {
          "type": "model",
          "model": "slimefun:slimefun/gui/_ui_previous_active"
        }
      },
      {
        "when": {"PublicBukkitValues":{"slimefun:slimefun_item":"_UI_NEXT_ACTIVE"}},
        "model": {
          "type": "model",
          "model": "slimefun:slimefun/gui/_ui_next_active"
        }
      },
      {
        "when": {"PublicBukkitValues":{"slimefun:slimefun_item":"IE__UI_PRODUCING"}},
        "model": {
          "type": "model",
          "model": "infinityexpansion2:icons/ui_producing"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/lime_stained_glass_pane"
    }
  },
  "oversized_in_gui": true
}